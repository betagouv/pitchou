{
    "include": ["scripts/types/customTypes.d.ts", "scripts/**/*.js"],
    "exclude": ["node_modules"],
    "compilerOptions": {
      "allowJs": true,
      "checkJs": true,
      "noEmit": true,
      "module": "esnext",
      "strict": true,
      "skipLibCheck": true,
      "moduleResolution": "bundler",
      "target": "esnext",
      "lib": ["es2022", "dom", "dom.iterable"],
      "downlevelIteration": true,
      "types": [],
      "noUnusedLocals": true,
      "noUnusedParameters": true,
      "allowImportingTsExtensions": true
    }
}