{
    "include": [
      "scripts/**/*.js", 
      "scripts/**/*.ts", 
      "scripts/**/*.d.ts",
      "outils/**/*.js"
    ],
    "exclude": [
      "node_modules", 
      "scripts/import-dossiers-historiques/nouvelle-aquitaine/**/*.js",
      "scripts/front-end/routes/import-historique/NouvelleAquitaine.js",
      "scripts/front-end/components/screens/ImportHistoriqueNouvelleAquitaine.svelte"
    ],
    "compilerOptions": {
      "allowJs": true,
      "checkJs": true,
      "allowImportingTsExtensions": true,
      "noEmit": true,
      "module": "esnext",
      "strict": true,
      "skipLibCheck": true,
      "moduleResolution": "bundler",
      "target": "esnext",
      "lib": ["es2023", "dom", "dom.iterable"],
      "downlevelIteration": true,
      "types": [],
      "noUnusedLocals": true,
      "noUnusedParameters": true
    }
}