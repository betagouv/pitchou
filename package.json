{
  "name": "pitchou",
  "version": "1.0.0",
  "type": "module",
  "engines": {
    "node": "20"
  },
  "scripts": {
    "build": "rollup -c",
    "dev": "npm-run-all --parallel dev:* start",
    "dev:rollup": "rollup -c -w",
    "start": "UID=\"$(id -u)\" GID=\"$(id -g)\" docker compose up",
    "prestart:server": "knex migrate:latest --env docker_dev",
    "start:server": "node --env-file=.env --watch scripts/server/main.js",
    "prestart:prod-server": "knex migrate:latest",
    "start:prod-server": "node scripts/server/main.js",
    "build-db-types": "docker exec node_server npx kanel -d postgresql://dev:dev_password@postgres_db:5432/principale -o ./scripts/types/database && ts-to-jsdoc --force ./scripts/types/database/public/*.ts",
    "migrate:up": "docker exec node_server npx knex migrate:up --env docker_dev",
    "migrate:down": "docker exec node_server npx knex migrate:down --env docker_dev"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-terser": "^0.4.4",
    "@types/d3-fetch": "^3.0.7",
    "@types/node": "^22.3.0",
    "@types/page": "^1.11.9",
    "kanel": "^3.8.8",
    "npm-run-all": "^4.1.5",
    "rollup": "^4.18.0",
    "rollup-plugin-css-only": "^4.5.2",
    "rollup-plugin-svelte": "^7.1.6",
    "sass": "^1.58.3",
    "simple-svelte-autocomplete": "^2.5.2",
    "svelte": "^4.2.19",
    "svelte-check": "^3.8.5",
    "svelte-preprocess": "^5.1.3",
    "ts-to-jsdoc": "^2.1.0",
    "typescript": "^5.5.3"
  },
  "dependencies": {
    "@fastify/static": "^7.0.1",
    "@gouvfr/dsfr": "^1.11.2",
    "baredux": "github:DavidBruant/baredux#v1.0.8",
    "csv-parse": "^5.5.6",
    "csv-stringify": "^6.5.0",
    "d3-fetch": "^3.0.1",
    "date-fns": "^3.6.0",
    "fastify": "^4.26.2",
    "knex": "^3.1.0",
    "ky": "^1.4.0",
    "minimist": "^1.2.8",
    "ods-xlsx": "github:DavidBruant/ods-xlsx#v0.8.0",
    "page": "^1.11.6",
    "pg": "^8.11.5",
    "pino-pretty": "^11.2.2",
    "remember": "github:DavidBruant/remember#v1.0.2"
  }
}
